{
  "$schema": "https://schema.railpack.com",
  "steps": {
    "install": {
      "commands": ["..."]
    },
    "build": {
      "commands": [
        "uv add -r requirements/production.txt",
        "uv run manage.py collectstatic --noinput",
        "uv run manage.py migrate"
      ]
    }
  },
  "deploy": {
    "startCommand": "gunicorn config:wsgi"
  }
}
